"amixer set Master 10%+"
  XF86AudioRaiseVolume

"amixer set Master 10%-"
  XF86AudioLowerVolume

"amixer set Master toggle"
  XF86AudioMute

"keyboardLight --increment"
  XF86KbdBrightnessUp

"keyboardLight --decrement"
  XF86KbdBrightnessDown

"displayLight --increment"
  XF86MonBrightnessUp

"displayLight --decrement"
  XF86MonBrightnessDown
