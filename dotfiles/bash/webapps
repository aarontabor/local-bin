function webapp {
  (nohup google-chrome-stable --app="$1" &) &> /dev/null
}
alias dropbox='webapp "https://www.dropbox.com"'
alias facebook='webapp "https://www.facebook.com"'
alias gdrive='webapp "https://drive.google.com"'
alias github='webapp "https://www.github.com"'
alias myunb='webapp "https:my.unb.ca/group/mycampus/home"'
alias uva='webapp "http://uva.onlinejudge.org"'
alias youtube='webapp "https://www.youtube.com"'
alias soundcloud='webapp "https://soundcloud.com"'
